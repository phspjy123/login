<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <link rel="stylesheet" href="/css/home/login.css">
    <script src="/js/home/register.js" defer></script>
    <title>Document</title>
</head>
<body>
    <div class="login-page">
        
      </div>
      <div class="login-box">
        <h2>Login</h2>
        <form>
          <div class="user-box">
            <input id="id" type="text">
            <label>ID</label>
          </div>
          <div class="user-box">
            <input id="name" type="password">
            <label>NAME</label>
          </div>
          <div class="user-box">
            <input id="psword" type="password">
            <label>Password</label>
          </div>
          <div class="user-box">
            <input id="confirm-psword" type="password">
            <label>confirm-psword</label>
          </div>
          <p id="button">SIGN UP</p>
        </form>
      </div>
</body>
</html>

